{
  "name": "technical-interview-exercise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest run --no-threads",
    "test:coverage": "vitest run --coverage",
    "pretest:a11y": "npm run build && npm run start",
    "test:a11y": "pa11y http://localhost:3000",
    "check-type": "tsc --noEmit",
    "server": "json-server --watch src/server/db.json --port 3001 --middlewares ./src/server/auth.js"
  },
  "dependencies": {
    "next": "14",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "^14",
    "json-server": "^0.17.4",
    "jsdom": "^26.0.0",
    "pa11y": "^8.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
