{
  "name": "technical-interview-exercise",
  "version": "0.1.0",
  "private": true,
  "module": "ESNext",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "pa11y http://localhost:3000 # The server should be available at this address before this command is run",
    "check-type": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "server": "json-server --watch src/server/db.json --port 3001 --middlewares ./src/server/auth.js"
  },
  "dependencies": {
    "next": "^15.2.2",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/eslint-plugin": "^1.1.37",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "json-server": "^0.17.4",
    "pa11y": "^8.0.0",
    "styled-components": "^6.1.15",
    "stylis": "^4.3.6",
    "typescript": "5.8.2",
    "vitest": "^3.0.8"
  }
}
