{
  "name": "technical-interview-exercise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest run",
    "test:coverage": "vitest run --coverage",
    "pretest:a11y": "npm run build && npm run start",
    "test:a11y": "pa11y http://localhost:3000",
    "check-type": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "server": "json-server --watch src/server/db.json --port 3001 --middlewares ./src/server/auth.js"
  },
  "dependencies": {
    "next": "^14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@next/eslint-plugin-next": "^15.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/eslint-plugin": "^1.1.37",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "json-server": "^0.17.4",
    "pa11y": "^8.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.x",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  }
}
